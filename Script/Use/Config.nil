class TConfig
	bool ColorBlind
	bool Tutorials
	string GameJoltUser
	string GameJoltToken
	
	void Store()
		SuperGlobal.Config = self[".dump"]
	end
	
	private void Retrieve()
		self[".dump"] = SuperGlobal.Config
	end
	
	void CONSTRUCTOR()
		self.ColorBlind = false
		self.Tutorials = true
		if SuperGlobal.Config!="" and SuperGlobal.Config!="nil"
			self.Retrieve()
		end
	end
		
end


return TConfig.NEW()

